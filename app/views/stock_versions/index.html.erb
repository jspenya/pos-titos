<h1>Stocks</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Product</th>
      <th scope="col">Quantity</th>
      <th scope="col">Histories</th>
    </tr>
  </thead>
  <tbody id="stocks">
    <% @stocks.each do |stock| %>
      <%= render "stock", stock: stock %>
    <% end %>
  </tbody>
</table>
