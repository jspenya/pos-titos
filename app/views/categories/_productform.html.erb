
<%= form_with model: [ @category, @category.products.build ] do |form| %>
    
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :quantity %>
  <%= form.number_field :quantity %>

  <%= form.label :price %>
  <%= form.number_field :price %>

 
  <%= form.button 'Add Product', class: 'btn btn-dark'%>
<% end %>